<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetProductServlet</servlet-name>
  	<display-name>GetProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.servlet.GetProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<display-name>UserLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<display-name>DeleteProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.servlet.DeleteProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProductServlet</servlet-name>
  	<display-name>EditProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.servlet.EditProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetProductServlet</servlet-name>
  	<url-pattern>/getproductpath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<url-pattern>/userloginpath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logoutpath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<url-pattern>/deleteproductpath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProductServlet</servlet-name>
  	<url-pattern>/editproductpath</url-pattern>
  </servlet-mapping>
</web-app>
